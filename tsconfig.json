{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es2016", "dom", "es2017.object"
    ],
    "outDir": "build",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true
  },
  "exclude": [
    "node_modules"
  ]
}
